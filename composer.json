{
    "name": "mixtape/mixtape",
    "description": "Mixtape: A php remix framework.",
    "license": "MIT",
    "authors": [
        {
            "name": "Nick alston",
            "email": "nick@mixtapephp.com"
        }
    ],
    "require": {
        "mixtape/framework": "dev-master",
        "filp/whoops": "^2.1",
        "guzzlehttp/guzzle": "~6.0"

    },
    "autoload":{
        "psr-4": {
            "App\\": "app/",
            "Http\\Controllers\\": "app/controllers/",
            "Guzzle\\": "vendor/guzzlehttp"

        },
        "classmap":[
            "CustomMixtapes",
            "vendor/mixtape/framework/src/Mixtapes",
            "vendor/filp/whoops/src/Whoops",
            "app/Exceptions"
        ]
    }
}
